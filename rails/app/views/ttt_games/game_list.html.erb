<h1>Select A Game</h1>
<table style="width: 640; margin: auto;">
  <tr>
    <% @games.each_with_index do |id, index| %>
      <% if index % 10 == 0 %>
        </tr>
        <tr>
      <% end %>
      <td style="width: 50px; height: 50px; text-align: right;"><a href="/ttt_games/<%=id%>/get_game"><%= id %></td>
    <% end %>
  </tr>
</table>
